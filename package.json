{
  "name": "cmdb",
  "version": "1.0.0",
  "main": "./server.js",
  "scripts": {
    "setenv": "source ../config/.env&&source .env",
    "prebuild": "yarn install&&rm -rf build",
    "buildEssential": "npm run prebuild&&EDITION=essential webpack",
    "build": "webpack",
    "buildDockerImage": "rm -rf Dockerfile&&ln ../config/deploy/Dockerfile .&&npm run prebuild&&EDITION=docker webpack",
    "test": "mocha",
    "start": "node server.js",
    "init": "initSchema",
    "initES": "INIT_ES=1 initSchema",
    "dockerInit": "docker exec cmdb initSchema",
    "exportToJson": "node exportJSON.js",
    "importFromJson": "IMPORT_FOLDER=./export_data/20170927095432 node ./importJSON.js",
    "importToESFromJson": "IMPORT_FOLDER=./export_data/20170927095432 IMPORT_STRATEGY=search node ./importJSON.js",
    "generateLicense": "generateLicense",
    "dockerStartDB":"docker-compose -f ../config/deploy/db.yml up -d",
    "dockerStopDB":"docker-compose -f ../config/deploy/db.yml stop",
    "dockerStartApp": "docker-compose -f ../config/deploy/app.yml up -d cmdb",
    "dockerStopApp": "docker-compose -f ../config/deploy/app.yml stop cmdb",
    "upgradeMemberOfRel": "cat ./cypher/buildMemberOfRel.cyp | $NEO4J_HOME/bin/cypher-shell -u neo4j -p admin --format plain"
  },
  "author": "Ronyang <yrong1997@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "config": "^1.25.1",
    "elasticsearch": "^12.1.3",
    "jsonfile": "^2.4.0",
    "jsonpath": "^1.0",
    "koa-neo4j-fork": "1.2.8-beta.3",
    "license-helper": "^1.0.4",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "moment": "^2.13.0",
    "qr-image": "^3.2.0",
    "redis-json-schema": "1.0.2-beta.1",
    "redis-json-schema-init": "1.0.1-beta.5",
    "request": "^2.81.0",
    "request-promise": "^4.1.1",
    "scirichon-acl-checker": "1.0.1-beta.5",
    "scirichon-cache": "1.0.4-beta.9",
    "scirichon-common": "1.0.1-beta.8",
    "scirichon-response-mapper": "1.0.0-beta.1",
    "scirichon-response-wrapper": "1.0.0-beta.8",
    "scirichon-token-checker": "1.0.0-beta.9",
    "uuid": "^3.0.0",
    "uuid-validate": "0.0.2"
  },
  "devDependencies": {
    "webpack-builder-advanced": "1.0.0-beta.2"
  }
}
