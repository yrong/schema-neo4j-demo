{
  "name": "cmdb",
  "version": "1.0.0",
  "main": "./server.js",
  "scripts": {
    "buildDockerImage": "EDITION=docker webpack",
    "buildEssential": "EDITION=essential webpack",
    "build": "webpack",
    "test": "mocha",
    "start": "NODE_ENV=production NODE_NAME=cmdb node ./server.js",
    "init": "/bin/bash ./script/init.sh",
    "initES": "/bin/bash ./script/init_es.sh",
    "backup": "/bin/bash ./script/backup.sh",
    "restore": "/bin/bash ./script/restore.sh --dir=./export_data/20170927091458",
    "exportToJson": "node ./exportJSON.js",
    "importFromJson": "IMPORT_FOLDER=./export_data/20170927095432 node ./importJSON.js",
    "importToESFromJson": "IMPORT_FOLDER=./export_data/20170927095432 IMPORT_STRATEGY=search node ./importJSON.js",
    "generateLicense": "generateLicense",
    "setenv": "source ../config/config/setenv.sh",
    "dockerStartDB":"docker-compose -f ../config/config/db.yml up -d",
    "dockerStopDB":"docker-compose -f ../config/config/db.yml stop",
    "dockerStartApp": "export CMDB_VERSION=`git rev-parse HEAD` && export NODE_ENV='development' && docker-compose -f ../config/config/app.yml up -d cmdb",
    "dockerStopApp": "export CMDB_VERSION=`git rev-parse HEAD` && docker-compose -f ../config/config/app.yml stop cmdb"
  },
  "author": "Ronyang <yrong1997@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "license-helper": "^1.0.4",
    "config": "^1.25.1",
    "elasticsearch": "^12.1.3",
    "jsonfile": "^2.4.0",
    "jsonpath": "^0.2.11",
    "koa-neo4j-fork": "1.2.8-beta.2",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "moment": "^2.13.0",
    "qr-image": "^3.2.0",
    "request": "^2.81.0",
    "request-promise": "^4.1.1",
    "scirichon-acl-checker": "1.0.1-beta.4",
    "scirichon-cache": "1.0.3-beta.4",
    "scirichon-common": "1.0.1-beta.6",
    "scirichon-response-wrapper": "1.0.0-beta.6",
    "scirichon-token-checker": "1.0.0-beta.6",
    "uuid": "^3.0.0",
    "uuid-validate": "0.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "git-revision-webpack-plugin": "^2.4.1",
    "mocha": "^3.2.0",
    "newman": "^3.8.3",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^2.3.2",
    "webpack-shell-plugin": "^0.5.0"
  }
}
