{
  "id": "/ConfigurationItem",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "it_service": {
      "type": "array",
      "items": { "type": "string" },
      "uniqueItems": true
    },
    "monitored": {
      "type": "boolean",
      "default": false
    },
    "responsibility":{
      "type": "integer"
    },
    "updated_by":{
      "type": "integer"
    },
    "technical_support_info": {
      "type": "string"
    },
    "status":{
      "type":"object",
      "properties":{
        "status":{
          "type": "string"
        }
      }
    }
  },
  "required": ["name","monitored"]
}
